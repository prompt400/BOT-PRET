{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "providers": ["node"],
      "phases": {
        "install": {
          "dependsOn": ["setup"],
          "cmds": [
            "npm install --save-exact"
          ]
        }
      }
    }
  },
  "deploy": {
    "startCommand": "node BOT-PRET-main/src/index.js",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3
  }
}
