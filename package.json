{
  "name": "discord-bot-pro",
  "version": "2.0.0",
  "description": "Bot Discord professionnel 2025",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "discord.js": "14.14.2",
    "dotenv": "16.4.2",
    "winston": "3.12.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.11.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "jest": "29.7.0",
    "nodemon": "3.0.3",
    "prettier": "3.2.0",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "22.10.0",
    "npm": "11.4.2"
  }
}
